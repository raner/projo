    aria-posinset       %Number;                                        #IMPLIED
    aria-setsize        %Number;                                        #IMPLIED
    aria-expanded       (true | false | undefined)  #IMPLIED
    aria-checked        CDATA                                               #IMPLIED
    aria-selected       (true | false | undefined)  #IMPLIED
  >

<!-- a multiline plain text edit control for the element's raw value -->
<!ELEMENT textarea (#PCDATA)>     
<!ATTLIST textarea
  %attrs;
  autocomplete                      (on | off)              #IMPLIED
  autofocus                             (autofocus)             #IMPLIED
  cols                                      %Number;                    #IMPLIED
  dirname                                   CDATA                           #IMPLIED
  disabled                              (disabled)              #IMPLIED
  form                                      IDREF                           #IMPLIED
    inputmode                               CDATA                           #IMPLIED
    maxlength                               %Length;                    #IMPLIED
    minlength                               %Length;                    #IMPLIED
  name                                      NMTOKEN                     #IMPLIED
    placeholder                         %Text;                      #IMPLIED
  readonly                              (readonly)              #IMPLIED
  required                              (required)              #IMPLIED
  rows                                      %Number;                    #IMPLIED
    wrap                                        (wrap)                      #IMPLIED
    aria-activedescendant       CDATA                           #IMPLIED
    aria-autocomplete               CDATA                           #IMPLIED
    aria-multiline                  (true | false)      #IMPLIED
    aria-readonly                       (true | false)      #IMPLIED
    aria-required                       (true | false)      #IMPLIED
  >

<!-- a key pair generator control -->
<!ELEMENT keygen EMPTY>
<!ATTLIST keygen
  %attrs;
    autofocus                               (autofocus)             #IMPLIED
    challenge                               CDATA                           #IMPLIED
  disabled                              (disabled)              #IMPLIED
  form                                      IDREF                           #IMPLIED
  keytype                                   CDATA                           #IMPLIED
  name                                      NMTOKEN                     #IMPLIED
  >

<!-- the result of a calculation or user action -->
<!ELEMENT output %Phrasing;>
<!ATTLIST output
  %attrs;
  for                                           %Text;                          #IMPLIED
  form                                      IDREF                               #IMPLIED
  name                                      NMTOKEN                         #IMPLIED
    aria-expanded       (true | false | undefined)  #IMPLIED
  >

<!-- the completion progress of a task -->
<!ELEMENT progress %progress.content;>
<!ATTLIST progress
  %attrs;
    max                                         %Number;                    #IMPLIED
  value                                     CDATA                           #IMPLIED
    aria-valuemax                       %Number;                    #IMPLIED
    aria-valuemin                       %Number;                    #IMPLIED
    aria-valuenow                       %Number;                    #IMPLIED
    aria-valuetext                  %Text;                      #IMPLIED
  >

<!-- a scalar measurement within a known range, or a fractional value -->
<!ELEMENT meter %meter.content;>
<!ATTLIST meter
  %attrs;
  value                                     CDATA                           #IMPLIED
    min                                         %Number;                    #IMPLIED
    max                                         %Number;                    #IMPLIED
    low                                         %Number;                    #IMPLIED
    high                                        %Number;                    #IMPLIED
    optimum                                 %Number;                    #IMPLIED
  >

<!-- a set of form controls optionally grouped under a common name -->
<!ELEMENT fieldset %fieldset.content;>
<!ATTLIST fieldset
  %attrs;
  disabled                              (disabled)                  #IMPLIED
  form                                      IDREF                               #IMPLIED
  name                                      NMTOKEN                         #IMPLIED
    role                                        %Text;                          #IMPLIED
    aria-activedescendant       CDATA                               #IMPLIED
    aria-expanded       (true | false | undefined)  #IMPLIED
  >

<!-- a caption for the rest of the contents of the fieldset -->
<!ELEMENT legend %Phrasing;>
<!ATTLIST legend
  %attrs;
    role                                        %Text;                          #IMPLIED
  >

<!--=================== 4.11 Scripting ====================================-->

<!-- allows authors to include dynamic script and data blocks in their documents -->
<!ELEMENT script (#PCDATA)>
<!ATTLIST script
  %attrs;
  src                                           %URI;                           #IMPLIED
  type                                      %ContentType;           #IMPLIED
  charset                                   %Charset;                   #IMPLIED
  async                                     (async)                     #IMPLIED
  defer                                     (defer)                     #IMPLIED
  crossorigin   (anonymous | use-credentials)   #IMPLIED
  >

<!-- displays its children if scripting is disabled. -->
<!ELEMENT noscript %Palpable;>
<!ATTLIST noscript
  %attrs;
  >

<!-- used to declare fragments of HTML that can be cloned and inserted in the document by script -->
<!ELEMENT template %template.content;>
<!ATTLIST template
  %attrs;
  >

<!-- provides scripts with a resolution-dependent bitmap canvas -->
<!ELEMENT canvas (#PCDATA)>
<!ATTLIST canvas
  %attrs;
    role                                        %Text;                      #IMPLIED
  height                                    %Length;                    #IMPLIED
  width                                     %Length;                    #IMPLIED
  >
