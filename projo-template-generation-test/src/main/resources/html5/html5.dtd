<!--================== Imported Names ====================================-->

<!ENTITY % ContentType "CDATA">
    <!-- media type, as per [RFC2045] -->

<!ENTITY % ContentTypes "CDATA">
    <!-- comma-separated list of media types, as per [RFC2045] -->

<!ENTITY % Charset "CDATA">
    <!-- a character encoding, as per [RFC2045] -->

<!ENTITY % Charsets "CDATA">
    <!-- a space separated list of character encodings, as per [RFC2045] -->

<!ENTITY % LanguageCode "NMTOKEN">
    <!-- a language code, as per [RFC3066] -->

<!ENTITY % Character "CDATA">
    <!-- a single character, as per section 2.2 of [XML] -->

<!ENTITY % Number "CDATA">
    <!-- one or more digits -->

<!ENTITY % LinkTypes "CDATA">
    <!-- space-separated list of link types -->

<!ENTITY % MediaDesc "CDATA">
    <!-- single or comma-separated list of media descriptors -->

<!ENTITY % URI "CDATA">
    <!-- a Uniform Resource Identifier, see [RFC2396] -->

<!ENTITY % UriList "CDATA">
    <!-- a space separated list of Uniform Resource Identifiers -->

<!ENTITY % Datetime "CDATA">
    <!-- date and time information. ISO date format -->

<!ENTITY % Script "CDATA">
    <!-- script expression -->

<!ENTITY % StyleSheet "CDATA">
    <!-- style sheet data -->

<!ENTITY % Text "CDATA">
    <!-- used for titles etc. -->

<!ENTITY % Length "CDATA">
    <!-- nn for pixels or nn% for percentage length -->

<!ENTITY % MultiLength "CDATA">
    <!-- pixel, percentage, or relative -->

<!ENTITY % Pixels "CDATA">
    <!-- integer representing length in pixels -->

<!-- these are used for image maps -->
<!ENTITY % Shape "(rect|circle|poly|default)">

<!ENTITY % Coords "CDATA">
    <!-- comma separated list of lengths -->
    

<!--=================== Global Attributes ===============================-->

<!-- attributes are common to and may be specified on all HTML elements
 accesskey              accessibility key character
 class                  space separated list of classes
 contenteditable    makes document region editable
 dir                            direction for weak/neutral text
 lang                           language code (backwards compatible)
 hidden                     do not render element
 id                             document-wide unique id
 spellcheck             check spelling and grammar 
 style                      associated style info
 tabindex                   position in tabbing order
 title                      advisory title/amplification
 translate              localise and element's attributes or its TEXT node children

-->
<!ENTITY % globalattrs
 "accesskey             %Character;             #IMPLIED
  class                     CDATA                           #IMPLIED
    contenteditable (contenteditable)   #IMPLIED
    dir                         (ltr|rtl)                   #IMPLIED
    hidden              (hidden)                    #IMPLIED
    id                          ID                              #IMPLIED
    lang                        %LanguageCode;      #IMPLIED
    spellcheck      (spellcheck)            #IMPLIED
  style                     %StyleSheet;            #IMPLIED
  tabindex              %Number;                    #IMPLIED
  title                     %Text;                      #IMPLIED
  translate         (yes | no)              #IMPLIED"
  >

<!-- The following event handler content attributes may be specified on any HTML element:
  onclick     a pointer button was clicked
  ondblclick  a pointer button was double clicked
  onmousedown a pointer button was pressed down
  onmouseup   a pointer button was released
  onmousemove a pointer was moved onto the element
  onmouseout  a pointer was moved away from the element
  onkeypress  a key was pressed and released
  onkeydown   a key was pressed down
  onkeyup     a key was released
-->
<!ENTITY % events
 "onabort                       %Script;    #IMPLIED
  onblur                        %Script;    #IMPLIED
  oncancel                  %Script;    #IMPLIED
  oncanplay                 %Script;    #IMPLIED
  oncanplaythrough  %Script;    #IMPLIED
  onchange                  %Script;    #IMPLIED
  onclick                       %Script;    #IMPLIED
  oncuechange               %Script;    #IMPLIED
  ondblclick                %Script;    #IMPLIED
  ondurationchange  %Script;    #IMPLIED
  onemptied                 %Script;    #IMPLIED
  onended                       %Script;    #IMPLIED
  onerror                       %Script;    #IMPLIED
  onfocus                       %Script;    #IMPLIED
  oninput                       %Script;    #IMPLIED
  oninvalid                 %Script;    #IMPLIED
  onkeypress                %Script;    #IMPLIED
  onkeydown                 %Script;    #IMPLIED
  onkeyup                       %Script;    #IMPLIED
  onload                        %Script;    #IMPLIED
  onloadeddata          %Script;    #IMPLIED
  onloadedmetadata  %Script;    #IMPLIED
  onloadstart               %Script;    #IMPLIED
  onmouseenter          %Script;    #IMPLIED
  onmouseleave          %Script;    #IMPLIED
  onmousedown               %Script;    #IMPLIED
  onmouseup                 %Script;    #IMPLIED
  onmouseover               %Script;    #IMPLIED
  onmousemove               %Script;    #IMPLIED
  onmouseout                %Script;    #IMPLIED
  onmousewheel          %Script;    #IMPLIED
  onpause                       %Script;    #IMPLIED
  onplay                        %Script;    #IMPLIED
  onplaying                 %Script;    #IMPLIED
  onprogress                %Script;    #IMPLIED
  onratechange          %Script;    #IMPLIED
  onreset                       %Script;    #IMPLIED
  onresize                  %Script;    #IMPLIED
  onscroll                  %Script;    #IMPLIED
  onseeked                  %Script;    #IMPLIED
  onseeking                 %Script;    #IMPLIED
  onselect                  %Script;    #IMPLIED
  onshow                        %Script;    #IMPLIED
  onstalled                 %Script;    #IMPLIED
  onsubmit                  %Script;    #IMPLIED
  onsuspend                 %Script;    #IMPLIED
  ontimeupdate          %Script;    #IMPLIED
  ontoggle                  %Script;    #IMPLIED
  onvolumechange        %Script;    #IMPLIED
  onwaiting                 %Script;    #IMPLIED"
  >

<!-- Media attributes introduced with HTML5
 preload            whether the browser should load the entire media file when the page loads
 autoplay       whether the media file should automatically start playing as soon as it can do so without stopping
 mediagroup   sets or returns the name of the media group the audio/video is a part of
 loop         specifies that the media file will start over again, every time it is finished.
 muted          specifies that the audio output of the video should be muted.
 controls       specifies that playback controls should be displayed.

-->
<!ENTITY % media
 "src                   %URI;                               #IMPLIED
  crossorigin (anonymous | use-credentials) #IMPLIED
  preload           (auto | metadata)       #IMPLIED
  autoplay    (autoplay)                    #IMPLIED
  mediagroup  (group)                           #IMPLIED
  loop        (loop)                            #IMPLIED
  muted         (muted)                         #IMPLIED
  controls    (controls)                    #IMPLIED"
  >
  
<!-- ARIA global states and properties can be used on any HTML element.
aria-atomic             present all, or only parts of, the changed region based on the change notifications. 
aria-busy                   whether an element, and its subtree, are currently being updated.
aria-controls           the element (or elements) whose contents or presence are controlled by the current element.
aria-describedby    the element (or elements) that describes the object.
aria-disabled           the element is perceivable but disabled, so it is not editable or otherwise operable.
aria-dropeffect     functions can be performed when the dragged object is released on the drop target.
aria-flowto             the next element (or elements) in an alternate reading order of content.
aria-grabbed            an element's "grabbed" state in a drag-and-drop operation.
aria-haspopup           the element has a popup context menu or sub-level menu.
aria-hidden             the element and all of its descendants are not visible or perceivable to any user 
aria-invalid            the entered value does not conform to the format expected by the application.
aria-label              a string value that labels the current element.
aria-labelledby     the element (or elements) that labels the current element.
aria-live                   an element will be updated, and describes the types of updates for the live region.
aria-owns                   define a visual, functional, or contextual parent/child relationship between DOM elements
aria-relevant           the user agent change notifications assistive technologies will receive within a live region

-->
<!ENTITY % aria.global
 "aria-atomic               (true | false)                          #IMPLIED
    aria-busy               (true | false)                          #IMPLIED
    aria-controls           CDATA                                               #IMPLIED
    aria-describedby    CDATA                                               #IMPLIED
    aria-disabled           (true | false)                          #IMPLIED
    aria-dropeffect     CDATA                                               #IMPLIED
    aria-flowto             CDATA                                               #IMPLIED
    aria-grabbed            (true | false | undefined)  #IMPLIED
    aria-haspopup           (true | false)                          #IMPLIED
    aria-hidden             (true | false)                          #IMPLIED
    aria-invalid            CDATA                                               #IMPLIED
    aria-label              %Text;                                          #IMPLIED
    aria-labelledby     CDATA                                               #IMPLIED
    aria-live                   CDATA                                               #IMPLIED
    aria-owns                   CDATA                                               #IMPLIED
    aria-relevant           CDATA                                               #IMPLIED"
>

<!-- HTML5 allows custom data-* attributes for all elements.
     Because the data-* elements are user defined, all we can do is add them here as they are encountered in code.
-->
<!ENTITY % data.attrs
 "  data-action       CDATA               #IMPLIED
    data-count      CDATA                       #IMPLIED
    data-chrome     CDATA                       #IMPLIED
    data-href         CDATA               #IMPLIED
    data-layout       CDATA               #IMPLIED
    data-link-color CDATA                      #IMPLIED
    data-parent     CDATA                    #IMPLIED
    data-show-count CDATA                       #IMPLIED
    data-show-faces   CDATA               #IMPLIED
    data-share        CDATA               #IMPLIED
    data-sitekey        CDATA               #IMPLIED
    data-target       CDATA             #IMPLIED
    data-toggle     CDATA                       #IMPLIED
    data-text       CDATA                          #IMPLIED
    data-url        CDATA                     #IMPLIED
    data-via        CDATA                       #IMPLIED
    data-widget-id  CDATA                   #IMPLIED"
>

<!-- Schema (Microdata) mark-up required additional custom attributes for all elements
     See schema.org for documentation
-->
<!ENTITY % schema.attrs
 "  itemscope       CDATA               #IMPLIED
    itemtype      CDATA                     #IMPLIED
    itemprop  CDATA                 #IMPLIED"
>

<!ENTITY % attrs "%globalattrs; %events; %aria.global; %data.attrs; %schema.attrs;">

<!--=================== Kinds of Content ====================================-->

<!-- Metadata content sets up the presentation or behavior of the rest of the content -->
<!ENTITY % metadata.content
    "base | link | meta | noscript | script | style | template | title">

<!-- Flow content includes most elements that are used in the body of documents and applications -->
<!ENTITY % flow.content
    "   a | abbr | area | audio | b | bdi | bdo |
        blockquote | br | canvas | cite | code | data | datalist | del | dfn | 
        div | dl | em | embed | fieldset | figure | hr | i | iframe | img | ins | kbd | label | 
        map | mark | math | noscript | object | ol | p |
        pre | q | ruby | s | samp | script | small | span |
        strong | sub | sup | svg | template | time | u | ul | var | 
        video | wbr">

<!-- Sectioning content defines the scope of headings and footers -->
<!ENTITY % sectioning.content
    "article | aside | nav | section">

<!-- Heading content defines the header of a section -->
<!ENTITY % heading.content
    "h1 | h2 | h3 | h4 | h5 | h6">

<!-- Phrasing content is the text of the document, as well as elements that mark up that text at the intra-paragraph level -->
<!ENTITY % phrasing.content
    "   abbr | area | b | bdi | bdo | br | canvas | cite | code | 
        data | datalist | del | em | i | ins | kbd | 
        map | mark | math | noscript | output | 
        q | ruby | s | samp | script | small | span | strong | sub | sup | svg |
        template | time | u | var | wbr">

<!-- Embedded content is content that imports another resource into the document, or content from another vocabulary that is inserted into the document -->
<!ENTITY % embedded.content
    "   audio | canvas | embed | iframe | img | math | object | svg | video">

<!-- Interactive content is content that is specifically intended for user interaction --> 
<!ENTITY % interactive.content
    "   a | audio | button | embed | iframe | img | input | keygen | label | object | select | textarea | video">

<!-- Palpable content includes elements any flow content or phrasing content that should have at least one node in its contents  -->
<!ENTITY %  palpable.content
    "   a | abbr | address | article | aside | audio | b | bdi | bdo | blockquote | button | 
        canvas | cite | code | data | dfn | div | dl | em | embed | fieldset | figure | 
        footer | form | h1 | h2 | h3 | h4 | h5 | h6 | header | i | iframe | img | input |
        ins | kbd | keygen | label | main | map | mark | math | meter | nav | object | ol |
        output | p | pre | progress | q | ruby | s | samp | section | select | small | span |
        strong | sub | sup | svg | table | textarea | time | u | ul | var |video">

<!-- Transparent content has a content model derived from that of its parent element (NOTE: Transparent also include Interactive content, not given here)  -->
<!ENTITY %  transparent.content
    "    abbr | address | article | aside | audio | b | bdi | bdo | blockquote | br  | canvas |
    cite | code | data | datalist | del | dfn | div | dl | em | fieldset | figure | footer |
    form | h1 | h2 | h3 | h4 | h5 | h6 | header | hr | i | img | ins | kbd | main | map | mark |
    math | meter | nav | noscript | object | ol | output | p | pre | progress | q | ruby | s |
    samp | script | section | small | span | strong | sub | sup | svg | table | template | time |
    u | ul | var | video | wbr">
    
<!-- Script-supporting elements are those that do not represent anything themselves (i.e. they are not rendered), but are used to support scripts -->
<!ENTITY % script.supporting
    "   script | template">

<!-- Labelable emements -->
<!ENTITY % labelable.elements
    "   button | input | keygen | meter | output | progress | select | textarea">

<!-- Labelable emements -->
<!ENTITY % table.elements
    "   table | caption | thead | tfoot | tbody | colgroup | col | tr | td | th">

<!--================== Content models =====================-->

<!ENTITY % Metadata "(%metadata.content;)*">

<!ENTITY % Flow "(#PCDATA | main | header | footer | form | address | table | hgroup | %flow.content; | %heading.content; | %sectioning.content; | %labelable.elements;)*">

<!ENTITY % Flow.no.main "(#PCDATA | header | footer | form | address | table | hgroup | %flow.content; | %heading.content; | %sectioning.content; | %labelable.elements;)*">

<!ENTITY % Flow.no.sections "(#PCDATA | form | address | table | %flow.content; | %heading.content; | %sectioning.content; | %labelable.elements;)*">

<!ENTITY % Flow.basic "(#PCDATA | address | table | form | %flow.content; | %labelable.elements;)*">

<!ENTITY % Sectioning "(%sectioning.content;)*">

<!ENTITY % Heading "(%flow.content;)*">

<!ENTITY % Heading.group "(%heading.content;)*">

<!ENTITY % Phrasing "(#PCDATA | dfn | progress | meter | %phrasing.content; | %interactive.content;)*">

<!ENTITY % Embedded "(%embedded.content;)*">

<!ENTITY % Interactive "(%interactive.content;)*">

<!ENTITY % Palpable "(%palpable.content;)*">

<!ENTITY % Transparent "(%transparent.content; | %interactive.content;)*">

<!ENTITY % Scripting "(%script.supporting;)*">


<!--================== Content model exclusions =====================-->


<!ENTITY % a.content "(#PCDATA | %transparent.content;)*">

<!ENTITY % address.content "(#PCDATA | %flow.content;)*">

<!ENTITY % dfn.content "(#PCDATA | %phrasing.content;)*">

<!ENTITY % media.content "(#PCDATA | track | source | %embedded.content;)*">

<!ENTITY % caption.content "(#PCDATA | %flow.content;)*">

<!ENTITY % form.content "(#PCDATA | address | table | %flow.content; | %heading.content; | %sectioning.content; | %labelable.elements;)*">

<!ENTITY % label.content "(#PCDATA  | dfn | progress | meter | %phrasing.content; | %interactive.content;)*">

<!ENTITY % button.content "(#PCDATA | img | %phrasing.content;)*">

<!ENTITY % progress.content "(#PCDATA | %phrasing.content;)*">

<!ENTITY % meter.content "(#PCDATA | %phrasing.content;)*">

<!ENTITY % fieldset.content "(legend? | table | %flow.content; | %heading.content; | %sectioning.content; | %labelable.elements;)*">

<!ENTITY % object.content "(#PCDATA | param | %transparent.content;)*">

<!ENTITY % figure.content "(figcaption | %flow.content;)*">

<!ENTITY % template.content "(%metadata.content; | %flow.content; | %heading.content; | %sectioning.content; | %labelable.elements; | %palpable.content; | %table.elements; )*">

<!ENTITY % picture.content "(#PCDATA | source | img)*">

<!--***********************************************************************-->
<!--**************** THE HTML5 ELEMENTS ***********************************-->
<!--***********************************************************************-->

<!--================ 4.1 The root element ==================================-->

<!-- the namespace URI designates the document profile -->

<!ELEMENT html (head, body)>
<!ATTLIST html
  %attrs;
  manifest      CDATA                       #IMPLIED
  >

<!--================ 4.2 Document metadata ===================================-->

<!ELEMENT head %Metadata;>
<!ATTLIST head
  %attrs;
   profile     %URI;          #IMPLIED
  >

<!-- The title element is not considered part of the flow of text.
       It should be displayed, for example as the page header or
       window title. Exactly one title is required per document.
    -->
<!ELEMENT title (#PCDATA)>
<!ATTLIST title 
  %attrs;
   >

<!-- document base URI -->

<!ELEMENT base EMPTY>
<!ATTLIST base
    %attrs;
    href        %URI;          #REQUIRED
  >

<!--
  Relationship values can be used in principle:

   a) for document specific toolbars/menus when used
      with the link element in document head e.g.
        start, contents, previous, next, index, end, help
   b) to link to a separate style sheet (rel="stylesheet")
   c) to make a link to a script (rel="script")
   d) by stylesheets to control how collections of
      html nodes are rendered into printed documents
   e) to make a link to a printable version of this document
      e.g. a PostScript or PDF version (rel="alternate" media="print")
-->
<!-- 
        NOTES:
        Non-standard 'sizes' attribute added for use by rel="apple-touch-icon"
-->
<!ELEMENT link EMPTY>
<!ATTLIST link
  %attrs;
  charset     %Charset;             #IMPLIED
  href        %URI;                     #IMPLIED
  hreflang    %LanguageCode;    #IMPLIED
  type        %ContentType;     #IMPLIED
  rel         %LinkTypes;           #IMPLIED
  rev         %LinkTypes;           #IMPLIED
  media       %MediaDesc;           #IMPLIED
  sizes             %Text;                  #IMPLIED
  aria-expanded     (true | false | undefined)  #IMPLIED
 >

<!-- generic metainformation -->
<!ELEMENT meta EMPTY>
<!ATTLIST meta
  %attrs;
  http-equiv  CDATA              #IMPLIED
  name        CDATA              #IMPLIED
  content     CDATA              #IMPLIED
  scheme      CDATA              #IMPLIED
  charset     %Charset;          #IMPLIED
  >
  
 <!-- allows authors to embed style information in their documents -->
<!ELEMENT style (#PCDATA)>
<!ATTLIST style
  %attrs;
  media             %MediaDesc;         #IMPLIED
  type              %ContentType;       #REQUIRED
  >


<!--=================== 4.3 Sections ====================================-->

<!ELEMENT body %Flow;>
<!ATTLIST body
  %attrs;
    onafterprint        %Script;    #IMPLIED
    onbeforeprint       %Script;    #IMPLIED
    onbeforeunload  %Script;    #IMPLIED
    onhashchange        %Script;    #IMPLIED
    onmessage               %Script;    #IMPLIED
    onoffline               %Script;    #IMPLIED
    ononline                %Script;    #IMPLIED
    onpagehide          %Script;    #IMPLIED
    onpageshow          %Script;    #IMPLIED
    onpopstate          %Script;    #IMPLIED
    onstorage               %Script;    #IMPLIED
    onunload                %Script;    #IMPLIED
  aria-expanded     (true | false | undefined)  #IMPLIED
 >

<!-- a complete, or self-contained, composition -->
<!ELEMENT article %Flow;>
<!ATTLIST article
  %attrs;
    role                        %Text;                                          #IMPLIED
  aria-expanded     (true | false | undefined)  #IMPLIED
  >

<!-- thematic grouping of content -->
<!ELEMENT section %Flow;>
<!ATTLIST section
  %attrs;
    role                        %Text;                                              #IMPLIED
  aria-expanded         (true | false | undefined)  #IMPLIED
  aria-orientation  CDATA                                               #IMPLIED
  >

<!-- a section with navigation links -->
<!ELEMENT nav %Flow;>
<!ATTLIST nav
  %attrs;
    role                        %Text;                                              #IMPLIED
  aria-expanded         (true | false | undefined)  #IMPLIED
  >

<!-- content that is tangentially related to the surrounding element content -->
<!ELEMENT aside %Flow.no.main;>
<!ATTLIST aside
  %attrs;
    role                        %Text;                                              #IMPLIED
  aria-expanded         (true | false | undefined)  #IMPLIED
  aria-orientation  CDATA                                               #IMPLIED
  >

<!--
  There are six levels of headings from h1 (the most important)
  to h6 (the least important).
-->

<!ELEMENT h1  %Phrasing;>
<!ATTLIST h1
    %attrs;
    role                    %Text;                                          #IMPLIED
    aria-expanded   (true | false | undefined)  #IMPLIED
    aria-selected   (true | false | undefined)  #IMPLIED
    aria-level      %Number;                                        #IMPLIED
   >

<!ELEMENT h2 %Phrasing;>
<!ATTLIST h2
    %attrs;
    role                    %Text;                                          #IMPLIED
    aria-expanded   (true | false | undefined)  #IMPLIED
    aria-selected   (true | false | undefined)  #IMPLIED
    aria-level      %Number;                                        #IMPLIED
  >

<!ELEMENT h3 %Phrasing;>
<!ATTLIST h3
    %attrs;
    role                    %Text;                                          #IMPLIED
    aria-expanded   (true | false | undefined)  #IMPLIED
    aria-selected   (true | false | undefined)  #IMPLIED
    aria-level      %Number;                                        #IMPLIED
  >

<!ELEMENT h4 %Phrasing;>
<!ATTLIST h4
    %attrs;
    role                    %Text;                                          #IMPLIED
    aria-expanded   (true | false | undefined)  #IMPLIED
    aria-selected   (true | false | undefined)  #IMPLIED
    aria-level      %Number;                                        #IMPLIED
  >

<!ELEMENT h5 %Phrasing;>
<!ATTLIST h5
    %attrs;
    role                    %Text;                                          #IMPLIED
    aria-expanded   (true | false | undefined)  #IMPLIED
    aria-selected   (true | false | undefined)  #IMPLIED
    aria-level      %Number;                                        #IMPLIED
  >

<!ELEMENT h6 %Phrasing;>
<!ATTLIST h6
    %attrs;
    role                    %Text;                                          #IMPLIED
    aria-expanded   (true | false | undefined)  #IMPLIED
    aria-selected   (true | false | undefined)  #IMPLIED
    aria-level      %Number;                                        #IMPLIED
  >

<!ELEMENT hgroup %Heading.group;>
<!ATTLIST hgroup
    %attrs;
    role                    %Text;                                          #IMPLIED
    aria-expanded   (true | false | undefined)  #IMPLIED
    aria-selected   (true | false | undefined)  #IMPLIED
    aria-level      %Number;                                        #IMPLIED
  >

<!-- group of introductory or navigational aids -->
<!ELEMENT header %Flow.no.sections;>
<!ATTLIST header
  %attrs;
    role                    %Text;                                          #IMPLIED
    aria-expanded   (true | false | undefined)  #IMPLIED
  >

<!-- information document sections such as who wrote it, links to related documents, copyright data, etc -->
<!ELEMENT footer %Flow.no.sections;>
<!ATTLIST footer
  %attrs;
    role                    %Text;                                          #IMPLIED
    aria-expanded   (true | false | undefined)  #IMPLIED
  >

<!-- contact information for its nearest article or body element ancestor -->
<!ELEMENT address %address.content;>
<!ATTLIST address
  %attrs;
    role                    %Text;                                          #IMPLIED
    aria-expanded   (true | false | undefined)  #IMPLIED
  >

<!--=================== 4.4 Grouping content =======================================-->

<!-- a paragraph -->
<!ELEMENT p %Phrasing;>
<!ATTLIST p
  %attrs;
    role                        %Text;  #IMPLIED
  >

<!-- paragraph-level thematic break -->
<!ELEMENT hr EMPTY>
<!ATTLIST hr
  %attrs;
    role                        %Text;  #IMPLIED
  aria-valuetext    %Text;  #IMPLIED
  >

<!-- block of preformatted text -->
<!-- content is %Inline; excluding "img|object|small|sub|sup" -->
<!ELEMENT pre %Phrasing;>
<!ATTLIST pre
  %attrs;
    role                        %Text;  #IMPLIED
 >

<!-- content that is quoted from another source -->
<!ELEMENT blockquote %Flow;>
<!ATTLIST blockquote
  %attrs;
  cite        %URI;       #IMPLIED
    role                %Text;          #IMPLIED
 >

<!-- Ordered (numbered) list -->
<!ELEMENT ol (li)+>
<!ATTLIST ol
  %attrs;
  reversed                              (reversed)                                  #IMPLIED
    start                                       %Number;                                        #IMPLIED
    type                                        CDATA                                               #IMPLIED
    role                                        %Text;                                          #IMPLIED
    aria-expanded                       (true | false | undefined)  #IMPLIED
    aria-multiselectable        (true | false)                          #IMPLIED
    aria-required                       (true | false)                          #IMPLIED
    aria-activedescendant       CDATA                                               #IMPLIED
    aria-level                          %Number;                                        #IMPLIED
  >

<!-- Unordered list -->
<!ELEMENT ul (li)+>
<!ATTLIST ul
  %attrs;
    role                                        %Text;                                          #IMPLIED
    aria-expanded                       (true | false | undefined)  #IMPLIED
    aria-multiselectable        (true | false)                          #IMPLIED
    aria-required                       (true | false)                          #IMPLIED
    aria-activedescendant       CDATA                                               #IMPLIED
    aria-level                          %Number;                                        #IMPLIED
 >


<!-- list item -->
<!ELEMENT li %Flow;>
<!ATTLIST li
  %attrs;
  value                                     %Number;                                        #IMPLIED
    role                                        %Text;                                          #IMPLIED
    aria-level                          %Number;                                        #IMPLIED
    aria-posinset                       %Number;                                        #IMPLIED
    aria-setsize                        %Number;                                        #IMPLIED
    aria-expanded                       (true | false | undefined)  #IMPLIED
    aria-checked                        CDATA                                               #IMPLIED
    aria-selected                       (true | false | undefined)  #IMPLIED
 >

<!-- definition lists - dt for term, dd for its definition -->

<!ELEMENT dl (dt|dd)+>
<!ATTLIST dl
  %attrs;
    role            %Text;      #IMPLIED
  >

<!ELEMENT dt %Flow.basic;>
<!ATTLIST dt
  %attrs;
    role            %Text;      #IMPLIED
  >

